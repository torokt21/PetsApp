<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PetsApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PetsApp"
    Shell.FlyoutBehavior="Flyout"
    Title="PetsApp">
    <FlyoutItem Title="Pet manager" Route="Main">
        
        <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Food"
        ContentTemplate="{DataTemplate local:FoodPage}"
        Route="FoodPage" />
    
    </FlyoutItem>
    <FlyoutItem>
        <ShellContent
     Title="Help"
     ContentTemplate="{DataTemplate local:HelpPage}"
     Route="HelpPage" />
    </FlyoutItem>
    


</Shell>
